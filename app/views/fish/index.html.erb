<p id="notice"><%= notice %></p>

<h1>Fish</h1>

<table>
  <thead>
    <tr>
      <th>Scientific</th>
      <th>English</th>
      <th>Lebanese</th>
      <th>Water</th>
      <th>Texture</th>
      <th>Flavor</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fish.each do |fish| %>
      <tr>
        <td><%= fish.scientific %></td>
        <td><%= fish.english %></td>
        <td><%= fish.lebanese %></td>
        <td><%= fish.water %></td>
        <td><%= fish.texture %></td>
        <td><%= fish.flavor %></td>
        <td><%= fish.image %></td>
        <td><%= link_to 'Show', fish %></td>
        <td><%= link_to 'Edit', edit_fish_path(fish) %></td>
        <td><%= link_to 'Destroy', fish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fish', new_fish_path %>
